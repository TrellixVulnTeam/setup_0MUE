{
    "label": "Autodesk Maya 2016x64",
    "description": "",
    "application_dir": "maya",
    "executable": "maya2016",
    "default_dirs": [
        "scenes",
        "data",
        "renderData/shaders",
        "images"
    ],
    "environment": {
        "MAYA_DISABLE_CIP": "Yes",
        "MAYA_DISABLE_CER": "Yes",
        "PYTHONPATH": [
            "{PYBLISH_MAYA}/pyblish_maya/pythonpath",
            "{MINDBENDER_CORE}/mindbender/maya/pythonpath",
            "{PYTHONPATH}"
        ]
    },
    "arguments": [
        "-proj",
        "{MINDBENDER_WORKDIR}"
    ],
    "copy": {
        "{MINDBENDER_CORE}/res/workspace.mel": "{MINDBENDER_WORKDIR}/workspace.mel"
    }
}
